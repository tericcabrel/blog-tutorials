{
  "name": "node-build-docker",
  "version": "1.0.0",
  "description": "REST API with all the endpoints documented built with Node.js and Express",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "nodemon --watch \"*.ts\" --exec \"ts-node\" ./src/index.ts",
